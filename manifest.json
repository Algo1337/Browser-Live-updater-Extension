{
    "manifest_version": 3,
    "name": "Form Interceptor",
    "version": "1.0",
    "description": "Intercepts form submissions and processes data.",
    "permissions": ["activeTab"],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  